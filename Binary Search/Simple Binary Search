public class Solution {
    public static int search(int []nums, int target) {
        int low = 0;
        int high = nums.length-1;

        while(low <= high){ //HAS TO GO TILL THE END
            int mid = (low + high)/2;

            if(nums[mid] == target){
                return mid;
            }

            THE MOST IMPORTANT PART
            else if(nums[mid] > target){
                high = mid-1;
            } else {
                low = mid+1;
            }
        }

        return -1;
    }
}
